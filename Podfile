platform :ios, '13.0'

def sharedPods

  # Networking
  pod 'Apollo', '0.43.0'

  # Storages
  pod 'KeychainSwift', '20.0'

  # Debaging
  pod 'CocoaDebug', '1.7.2', :configurations => ['Debug']

end

target 'finch-taxi-aleksandr (release)' do
  use_frameworks!

  sharedPods

end

target 'finch-taxi-aleksandr (debug)' do
  use_frameworks!

  sharedPods

end
